<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="DCDM.SwitchTemplate" table="SWITCH_TEMPLATE">
      <meta attribute="class-description">
         This class contains the switch template detail. 
      </meta>
      <id name="id" type="long" column="id">
         <generator class="increment"/>
      </id>
      <property name="switchType" column="switch_type" type="int"/>
      <property name="switchName" column="switch_name" type="int"/>
      <property name="hight" column="hight" type="float"/>
      <property name="width" column="width" type="float"/>
      <property name="depth" column="depth" type="float"/>
      <property name="location" column="location" type="string"/>
      <property name="NumberOfPorts" column="Number_of_ports" type="int"/>
   </class>
</hibernate-mapping>